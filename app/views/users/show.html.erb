<!-- <div class="row container">
  <div class="whitespace col-md-14"> -->
  <div class="row container">
    <div class="whitespace col-md-12">
    <br>
    </div>
    <div class="col-md-1">
      &nbsp;
    </div>
    <div class="col-md-8" style="text-align:center">
        <h1 id="username"><%= @user.name %></h1>
        </br>
<!--         <div id="points-div">
          <h1 id="users-points"><%= @users_points%></br></h1>
        </div> -->
        <h1><span class="glyphicon glyphicon-star-empty" aria-hidden="true">&nbsp;</span><span class="label label-default"> <%= @users_points%> points </span>&nbsp;&nbsp;<span class="glyphicon glyphicon-star-empty" aria-hidden="true"></span></h1>
        <br>
        <div class="progress">
          <div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: <%=@users_percentage%>%;">
            <%=@users_percentage%>%
          </div>
        </div>

      <br>
        <% if current_user.present? && @user == current_user then%>

          <div class="btn-group" role="group" aria-label="...">
            <%= link_to "Delete account", user_path(@user), method: :delete, class: "btn btn-default btn-lg", data: { confirm: "You sure?" } %>
            <%= link_to "Edit my account", edit_user_registration_path(@user), class: "btn btn-default btn-lg"%>
          </div>
        <%end%>
        <br>


  </div>
  <div class="col-md-3">
    <% if current_user.present? && @user == current_user then%>
      <h3>Recently done photos:</h3></br>
        <div id="nevus-profile-container">
          <% @photos_done.each do |photo| %>
            <%= image_tag(photo.picture_url, :class => "photo-log-in nevus-profile")%></br>
          <%end%>
        </div>
    <%end%>
  </div>
</div>








